  "ceterms:name": "Certified Safety Professional (CSP)",
  "ceterms:occupationType": [
    {
      "@type": "ceterms:CredentialAlignmentObject",
      "ceterms:codedNotation": "17-2111.00",
      "ceterms:framework": "https://www.bls.gov/soc/",
      "ceterms:frameworkName": "Standard Occupational Classification",
      "ceterms:targetNodeDescription": "Promote worksite or product safety by applying knowledge of industrial processes, mechanics, chemistry, psychology, and industrial health and safety laws. Includes industrial product safety engineers.",
      "ceterms:targetNodeName": "Health and Safety Engineers, Except Mining Safety Engineers and Inspectors"
    },
    {
      "@type": "ceterms:CredentialAlignmentObject",
      "ceterms:codedNotation": "17-2111.03",
      "ceterms:framework": "https://www.bls.gov/soc/",
      "ceterms:frameworkName": "Standard Occupational Classification",
      "ceterms:targetNodeDescription": "Develop and conduct tests to evaluate product safety levels and recommend measures to reduce or eliminate hazards.",
      "ceterms:targetNodeName": "Product Safety Engineers"
    },
    {
      "@type": "ceterms:CredentialAlignmentObject",
      "ceterms:codedNotation": "29-9011.00",
      "ceterms:framework": "https://www.bls.gov/soc/",
      "ceterms:frameworkName": "Standard Occupational Classification",
      "ceterms:targetNodeDescription": "Review, evaluate, and analyze work environments and design programs and procedures to control, eliminate, and prevent disease or injury caused by chemical, physical, and biological agents or ergonomic factors. May conduct inspections and enforce adherence to laws and regulations governing the health and safety of individuals. May be employed in the public or private sector. Includes environmental protection officers.",
      "ceterms:targetNodeName": "Occupational Health and Safety Specialists"
    },
    {
      "@type": "ceterms:CredentialAlignmentObject",
      "ceterms:codedNotation": "29-9012.00",
      "ceterms:framework": "https://www.bls.gov/soc/",
      "ceterms:frameworkName": "Standard Occupational Classification",
      "ceterms:targetNodeDescription": "Collect data on work environments for analysis by occupational health and safety specialists. Implement and conduct evaluation of programs designed to limit chemical, physical, biological, and ergonomic risks to workers.",
      "ceterms:targetNodeName": "Occupational Health and Safety Technicians"
    },
    {
      "@type": "ceterms:CredentialAlignmentObject",
      "ceterms:targetNodeName": "Safety Executive"
    },
